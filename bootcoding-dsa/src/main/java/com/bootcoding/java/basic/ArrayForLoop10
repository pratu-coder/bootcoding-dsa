package com.bootcoding.java.basic;

public class ArrayForLoop8 {
    public static void main(String[] args) {
        int[] arr = {10, 20, 30, 40};
        int max = 0;
        for (int i = 1; i < ele.length; i++) {
            if (arr[i] <= max) {
                System.out.println("max ele of array is " + max[i]);
            }
        }
    }
}

